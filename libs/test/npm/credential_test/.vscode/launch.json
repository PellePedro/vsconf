{
    "version": "0.2.0",
    "configurations": [
      // Javascript Generate
      {
        "name": "‚öôÔ∏è tester generate javascript",
        "type": "go",
        "request": "launch",
        "mode": "auto",
        "program": "${workspaceFolder}/../../../../cmd/skyramp",
        "cwd": "${workspaceFolder}",
        "args": [
          "tester",
          "generate",
          "rest",
          "--alias",
          "infra",
          "--paths",
          "/apis/infra.k8smgmt.io/v3/projects/{project}/credentials",
          "--api-schema",
          "https://qc-console.stage.rafay.dev/apis/.well-known/openapi.json",
          "--sample-request",
          "data/credentials.json",
          "--port",
          "8080",
          "--language",
          "javascript"
        ],
      },
      {
        "name": "üöÄ run javascript crud test",
        "type": "node",
        "request": "launch",
        "program": "${workspaceFolder}/node_modules/.bin/playwright",
        "args": [
          "test",
          "--timeout=0",
          "${workspaceFolder}/infra.spec.js"
        ],
        "console": "integratedTerminal",
        "internalConsoleOptions": "neverOpen",
        "cwd": "${workspaceFolder}",
        "env": {
          "SKYRAMP_DEBUG": "true",
          "SKYRAMP_WORKER_ADDRESS": "localhost:45555",
          "SKYRAMP_TEST_NAME": "skyramp test",
          "SKYRAMP_SCENARIO_NAME": "test scenario"
        }
      }
    ]
  }